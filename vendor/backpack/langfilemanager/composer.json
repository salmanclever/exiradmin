{
    "name": "backpack/langfilemanager",
    "description": "An interface to edit language files, for Laravel Backpack.",
    "keywords": [
        "backpack",
        "laravel backpack",
        "lang files",
        "lang file manager",
        "language",
        "dick",
        "tabacitu",
        "updivision",
        "dick admin",
        "dickadmin",
        "laravel",
        "admin panel",
        "translationmanager",
        "dick translationmanager"
    ],
    "homepage": "https://github.com/laravel-backpack/langfilemanager",
    "license": "MIT",
    "authors": [
        {
            "name": "Alin Ghitu",
            "email": "alin@updivision.com",
            "role": "Lead Developer"
        },
        {
            "name": "Cristian Tabacitu",
            "email": "hello@tabacitu.ro",
            "homepage": "http://www.tabacitu.ro",
            "role": "Contributor"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "backpack/crud": "^3.0.6"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\LangFileManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\LangFileManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Backpack\\LangFileManager\\LangFileManagerServiceProvider"
            ]
        }
    }
}
